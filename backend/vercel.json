{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "your-mongodb-uri",
    "CLOUDINARY_URL": "your-cloudinary-url",
    "ANOTHER_ENV": "another-value"
  }
}
